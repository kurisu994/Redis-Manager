{
  "name": "redis-desktop-client",
  "private": true,
  "version": "0.0.1",
  "description": "A cross-platform redis gui client",
  "type": "module",
  "scripts": {
    "dev": "vite --host=localhost",
    "build": "tsc && vite build",
    "eslint": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix --cache",
    "stylelint": "stylelint 'src/**/*.less' 'src/**/*.css' --fix --cache",
    "postinstall": "husky install",
    "pre-commit": "pretty-quick --staged && npm run eslint && npm run stylelint",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev --features=redis-client-dev"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.47.1",
    "@icon-park/react": "^1.4.2",
    "@monaco-editor/react": "^4.5.1",
    "@tauri-apps/api": "^1.3.0",
    "ahooks": "^3.7.7",
    "clsx": "^1.2.1",
    "monaco-editor": "^0.38.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1"
  },
  "devDependencies": {
    "@arco-design/webpack-plugin": "^1.7.0",
    "@arco-plugins/vite-react": "^1.3.3",
    "@arco-themes/react-arco-pro": "^0.0.7",
    "@tauri-apps/cli": "^1.3.0",
    "@types/node": "^20.1.0",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5"
  },
  "license": "MIT"
}
